{
  "name": "CryoBase",
  "description": "Application for managing cryo-EM data on AWS S3.",
  "version": "1.0.6",
  "main": "public/electron.js",
  "homepage": "./",
  "private": true,
  "author": {
    "name": "Joel Meyerson"
  },
  "license": "MIT",
  "dependencies": {
    "@ant-design/icons": "^4.5.0",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "antd": "^4.12.3",
    "aws-sdk": "^2.843.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^4.0.2",
    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron-start": "export NODE_ENV=development && electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "postinstall": "electron-builder install-app-deps",
    "preelectron-pack": "bash prebuild.sh && npm run build",
    "electron-pack": "electron-builder -ml  && bash postbuild.sh"
  },
  "build": {
    "appId": "com.cryobase.app",
    "productName": "CryoBase",
    "mac": {
      "category": "public.app-category.utilities"
    },
    "dmg": {
      "background": null,
      "backgroundColor": "#ffffff",
      "window": {
        "width": "400",
        "height": "300"
      },
      "contents": [
        {
          "x": 110,
          "y": 100
        },
        {
          "x": 240,
          "y": 100,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ],
      "category": "Utility"
    },
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "electron": "^11.2.3",
    "electron-builder": "^22.9.1",
    "electron-reload": "^1.5.0"
  }
}
